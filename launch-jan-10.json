{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "python-debug-current-file",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "ship firefox target graph",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/mach",
      "args": [
        "taskgraph",
        "target-graph",
        "-p",
        "taskcluster/test/params/mr-ship-firefox.yml",
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "ship devedition target graph",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/mach",
      "args": [
        "taskgraph",
        "target-graph",
        "-p",
        "ship_devedition_parameters.yml",
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "release-update-verify-config-devedition-linux64-aarch64",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/mach",
      "args": [
        "python",
        "testing/mozharness/scripts/release/update-verify-config-creator.py",
        "--product",
        "firefox",
        "--stage-product",
        "devedition",
        "--app-name",
        "browser",
        "--branch-prefix",
        "mozilla",
        "--platform",
        "linux-aarch64",
        "--to-version",
        "140.0b8",
        "--to-app-version",
        "140.0",
        "--to-build-number",
        "1",
        "--to-buildid",
        "20250108214144",
        "--to-revision",
        "4a129c8c575fec5b230788bba27580d91c851702",
        "--last-linux-bz2-version",
        "133.0b9",
        "--output-file",
        "update-verify.cfg",
        "--local-repo",
        ".",
        "--repo-path",
        "try",
        "--partial-version",
        "140.0b6",
        "--updater-platform",
        "linux-aarch64",
        "--channel",
        "aurora-localtest",
        "--archive-prefix",
        "http://ftp.stage.mozaws.net/pub",
        "--previous-archive-prefix",
        "http://ftp.stage.mozaws.net/pub",
        "--aus-server",
        "https://stage.balrog.nonprod.cloudops.mozgcp.net",
        "--override-certs",
        "dep",
        "--include-version",
        "^(\\d+\\.\\d+(b\\d+)?)$",
        "--last-watershed",
        "140.0b6"
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
      }
    },
    {
      "name": "release-update-verify-firefox-linux64-aarch64-1/16",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/mach",
      "args": [
        "python",
        "${workspaceFolder}/tools/update-verify/scripts/chunked-verify.py",
        "--chunks",
        "16",
        "--this-chunk",
        "1",
        "--verify-config",
        "${workspaceFolder}/update-verify.cfg",
        "--diff-summary",
        "${workspaceFolder}/diff-summary.log",
        "--verify-channel",
        "beta-localtest"
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
      }
    },
    {
      "name": "release-update-verify-config-firefox-linux64-aarch64",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/mach",
      "args": [
        "python",
        "testing/mozharness/scripts/release/update-verify-config-creator.py",
        "--product",
        "firefox",
        "--stage-product",
        "firefox",
        "--app-name",
        "browser",
        "--branch-prefix",
        "mozilla",
        "--platform",
        "linux-aarch64",
        "--to-version",
        "140.0b8",
        "--to-app-version",
        "140.0",
        "--to-build-number",
        "1",
        "--to-buildid",
        "20250108214144",
        "--to-revision",
        "4a129c8c575fec5b230788bba27580d91c851702",
        "--last-linux-bz2-version",
        "133.0b9",
        "--output-file",
        "update-verify.cfg",
        "--local-repo",
        ".",
        "--repo-path",
        "try",
        "--partial-version",
        "140.0b6",
        "--updater-platform",
        "linux-aarch64",
        "--channel",
        "beta-localtest",
        "--archive-prefix",
        "http://ftp.stage.mozaws.net/pub",
        "--previous-archive-prefix",
        "http://ftp.stage.mozaws.net/pub",
        "--aus-server",
        "https://stage.balrog.nonprod.cloudops.mozgcp.net",
        "--override-certs",
        "dep",
        "--include-version",
        "^(\\d+\\.\\d+(b\\d+)?)$",
        "--mar-channel-id-override",
        "^\\d+\\.\\d+(\\.\\d+)?$$,firefox-mozilla-beta,firefox-mozilla-release",
        "--last-watershed",
        "140.0b6"
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
      }
    }
  ]
}
